prefix=${pcfiledir}/../..
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/../include

Name: absl_city
Description: Abseil city library
URL: https://abseil.io/
Version: 20240722

Libs: "-L${libdir}" -labsl_city
Requires: absl_config = 20240722, absl_core_headers = 20240722, absl_endian = 20240722
Cflags: "-I${includedir}" -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
