prefix=${pcfiledir}/../..
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: absl_cleanup
Description: Abseil cleanup library
URL: https://abseil.io/
Version: 20240722

Libs: "-L${libdir}"
Requires: absl_cleanup_internal = 20240722, absl_config = 20240722, absl_core_headers = 20240722
Cflags: "-I${includedir}" -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
